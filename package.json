{
  "name": "axios-api-gen",
  "description": "This package is generate API from path definition, it is based on axios and it can integrate perfectly with mswx(mock service worker extension).",
  "keywords": [
    "msw",
    "mswx",
    "axios",
    "API"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/LaiJunBin/axios-api-gen.git"
  },
  "author": "Lai JunBin",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/axios-api-gen.umd.cjs",
  "module": "./dist/axios-api-gen.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/axios-api-gen.js",
      "require": "./dist/axios-api-gen.umd.cjs"
    }
  },
  "version": "0.1.1",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "build": "tsc && vite build",
    "prepare": "husky install",
    "lint": "eslint --ext .ts ./src --fix"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.3",
    "mswx": "^0.4.0",
    "prettier": "^2.8.7",
    "typescript": "*",
    "vite": "^4.2.0",
    "vite-plugin-dts": "^2.2.0",
    "vitest": "^0.30.0"
  },
  "dependencies": {
    "axios": "^1.3.5"
  }
}
